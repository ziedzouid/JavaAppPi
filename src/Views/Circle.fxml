<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.text.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" styleClass="gradient" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CircleController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
   <children>
      <JFXButton fx:id="editcircle" layoutX="604.0" layoutY="508.0" onAction="#editcircle" prefHeight="64.0" prefWidth="144.0" style="-fx-background-color: #0b7384;" text="Edit" textFill="#0d0d0d">
         <font>
            <Font size="25.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="addcircle" layoutX="441.0" layoutY="508.0" onAction="#addcircle" prefHeight="64.0" prefWidth="144.0" style="-fx-background-color: #0b7384;" text="ADD" textFill="#151717">
         <font>
            <Font size="29.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="removecircle" layoutX="774.0" layoutY="508.0" onAction="#removecircle" prefHeight="64.0" prefWidth="144.0" style="-fx-background-color: #0b7384;" text="Remove" textFill="#0a0a0a">
         <font>
            <Font size="24.0" />
         </font>
      </JFXButton>
      <Label layoutX="62.0" layoutY="21.0" prefHeight="64.0" prefWidth="438.0" text="Circles List" textFill="#172a57">
         <font>
            <Font size="51.0" />
         </font>
      </Label>
      <JFXTextArea fx:id="namecircle" layoutX="748.0" layoutY="110.0" prefHeight="50.0" prefWidth="212.0" />
      <Label layoutX="656.0" layoutY="141.0" prefHeight="38.0" prefWidth="58.0" text="Name">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <JFXTextArea fx:id="typecircle" layoutX="748.0" layoutY="193.0" prefHeight="38.0" prefWidth="212.0" />
      <Label layoutX="649.0" layoutY="216.0" prefHeight="30.0" prefWidth="72.0" text="TYPE">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <JFXTextArea fx:id="descriptioncircle" layoutX="756.0" layoutY="255.0" prefHeight="64.0" prefWidth="197.0" />
      <Label layoutX="627.0" layoutY="313.0" text="DESCRIPTION">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <JFXListView fx:id="listcircle" layoutX="46.0" layoutY="106.0" onMouseClicked="#selectedcircle" prefHeight="374.0" prefWidth="410.0" />
      <Button fx:id="retcircle" layoutX="855.0" layoutY="23.0" mnemonicParsing="false" onAction="#returnfromcircle" prefHeight="27.0" prefWidth="72.0" text="Return">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
